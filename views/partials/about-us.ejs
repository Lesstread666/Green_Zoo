<body>
    <main>
        <section class="team-section">
            <% team.forEach(member=> { %>
                <div class="team-member">
                    <img src="<%= member.image %>" alt="<%= member.name %>">
                    <h2>
                        <%= member.name %>
                    </h2>
                    <p>
                        <%= member.description %>
                    </p>
                    <div class="social-links">
                        <a href="<%= member.social.github %>"><img src="/images/logos-icons/github-icon.png"
                                alt="Github"></a>
                        <a href="<%= member.social.vercel %>"><img src="/images/logos-icons/vercel-icon.png"
                                alt="Vercel"></a>
                    </div>
                </div>
                <% }) %>
        </section>
    </main>
</body>